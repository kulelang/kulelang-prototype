{
    "ecmaFeatures": {
        "modules": true,
        "spread" : true,
        "restParams" : true
    },
    "rules" : {
        "no-unused-vars" : 2,
        "no-undef" : 2
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "env" : {
        "browser" : true,
        "node" : true,
        "es6" : true
    },
    "plugins": [
        "vue"
    ]
}